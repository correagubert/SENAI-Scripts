programa {
  funcao inicio() {
    real n1, n2
    escreva("Insira o primeiro número: ")
    leia(n1)
    escreva("Insira o segundo número: ")
    leia(n2)
    se(n1>n2){
      escreva("O primeiro número é maior!")
    }senao{
    se(n1<n2){
      escreva("O segundo número é maior!")
    }senao{
    se(n1==n2){
      escreva("Os dois números são iguais!")
    }
    }
    }
  }
}
